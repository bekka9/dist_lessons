#include <algorithm>
#include <stack>
#include <iostream>
#include <vector>
using namespace std;


int main() {
   stack <int> s;
   int n;
   vector <int> v;
   cin >> n;
   for (int i=0; i<n; i++)
    s.push(1);
    int m;
    cin >> m;
   for (int j=0; j<m; j++)
   {
       
       s.pop();
       s.top()++;

   }
   while (!s.empty)
   {
       v.push_back(s.top());
       s.pop();
   }
   reverse (v.begin(),v.end());
  for (int i=0;i< v.size(); i++)
    cout << v[i] << "+";
  

}
