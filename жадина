#include <iostream>

using namespace std;

int main()
{
    int money[] = {10,8,2,1};
    int n = 4;
    int sum;
    cout << "enter a sum:";
    cin >> sum;
    int m = 0;
    for (int i = 0; i < n; i++)
    {
        m += sum/money[i];
        sum = sum%money[i];
    }
    
        cout << m;
    
    return 0;
}
